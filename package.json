{
  "name": "dermatology-ai-orchestrator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "syntax:check": "bash scripts/validate-syntax.sh",
    "test": "npx vitest run --config vitest.config.ts",
    "test:fast": "npx vitest run --config vitest.config.ts --threads 1 --reporter verbose",
    "test:ci": "npx vitest run --config vitest.config.ts --coverage --reporter=verbose",
    "ci": "npm run prettier:check && npm run lint && npm run typecheck && npm run test",
    "quality-gate": "bash scripts/quality-gate.sh",
    "quality-gate:fast": "bash scripts/quality-gate.sh --fast",
    "quality-gate:fix": "bash scripts/quality-gate.sh --fix",
    "bundle:analyze": "npm run build && npx webpack-bundle-analyzer dist/assets/*.js",
    "bundle:size": "size-limit",
    "bundle:budget": "npm run build && size-limit --why",
    "license:scan": "npx license-checker --summary",
    "security:audit": "npm audit --audit-level moderate",
    "apply:branch-protection": "bash scripts/apply-branch-protection.sh do-ops885/dermatology-goap-orchestrator main",
    "agentdb:init": "npx agentdb init"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@google/genai": "^1.34.0",
    "@mlc-ai/web-llm": "0.2.80",
    "@tensorflow/tfjs": "4.22.0",
    "@tensorflow/tfjs-backend-webgpu": "4.22.0",
    "@xenova/transformers": "2.17.1",
    "agentdb": "^1.6.1",
    "buffer": "6.0.3",
    "events": "3.3.0",
    "framer-motion": "^12.24.7",
    "long": "5.2.3",
    "lucide-react": "^0.562.0",
    "process": "latest",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "stream-browserify": "^3.0.0",
    "util": "0.12.5",
    "uuid": "^9.0.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.39.2",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.57.0",
    "@size-limit/preset-app": "^12.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.0.2",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.26.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^51.0.0",
    "globals": "^17.0.0",
    "husky": "^8.0.3",
    "jsdom": "^24.1.3",
    "license-checker": "^25.0.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "size-limit": "^12.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.52.0",
    "vite": "^6.2.0",
    "vitest": "^4.0.0",
    "webpack-bundle-analyzer": "^5.1.1",
    "zod": "^3.24.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "size-limit": [
    {
      "path": "dist/assets/index-*.js",
      "limit": "500 kB",
      "gzip": true,
      "name": "Main application bundle"
    },
    {
      "path": "dist/assets/vendor-*.js",
      "limit": "3 MB",
      "gzip": true,
      "name": "Vendor bundles (AI/ML models)"
    },
    {
      "path": "dist/assets/*.css",
      "limit": "100 kB",
      "gzip": true,
      "name": "Styles"
    }
  ]
}
