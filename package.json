{
  "name": "dermatology-ai-orchestrator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "npx vitest run --run",
    "test:fast": "npx vitest -c --run --threads 1 --reporter verbose",
    "ci": "npm run prettier:check && npm run lint && npm run typecheck && npm run test",
    "quality-gate": "bash scripts/quality-gate.sh",
    "quality-gate:fast": "bash scripts/quality-gate.sh --fast",
    "quality-gate:fix": "bash scripts/quality-gate.sh --fix",
    "apply:branch-protection": "bash scripts/apply-branch-protection.sh do-ops885/dermatology-goap-orchestrator main",
    "agentdb:init": "npx agentdb init"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@google/genai": "^1.34.0",
    "@mlc-ai/web-llm": "0.2.80",
    "@tensorflow/tfjs": "4.22.0",
    "@tensorflow/tfjs-backend-webgpu": "4.22.0",
    "@xenova/transformers": "2.17.2",
    "agentdb": "2.0.0-alpha.3.3",
    "buffer": "6.0.3",
    "events": "3.3.0",
    "framer-motion": "^12.24.7",
    "long": "5.2.3",
    "lucide-react": "^0.562.0",
    "process": "latest",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "stream-browserify": "^3.0.0",
    "util": "0.12.5",
    "uuid": "^9.0.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.39.2",
    "eslint": "^8.57.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.0.2",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.8",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^51.0.0",
    "globals": "^17.0.0",
    "husky": "^8.0.3",
    "jsdom": "^24.1.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.52.0",
    "vite": "^6.2.0",
    "vitest": "^2.1.9"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  }
}
